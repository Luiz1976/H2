# Supabase Configuration
# IMPORTANTE: Nunca commite o arquivo .env - ele já está no .gitignore

# URL do projeto Supabase
VITE_SUPABASE_URL=https://seu-projeto.supabase.co

# Chave pública anônima do Supabase
VITE_SUPABASE_ANON_KEY=sua_chave_anonima_aqui

# Chave de serviço do Supabase (apenas para scripts backend)
SUPABASE_SERVICE_ROLE_KEY=sua_service_role_key_aqui

# API Backend Configuration
# URL base da API backend segura
VITE_API_BASE_URL=http://localhost:3001

# Chave secreta para autenticação na API
VITE_API_SECRET_KEY=sua_api_secret_key_aqui